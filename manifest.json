{
  "manifest_version": 3,
  "name": "GPT Star Test",
  "version": "1.0",
  "description": "تست نمایش یک PNG روی صفحه",
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/star-empty.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
